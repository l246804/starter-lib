{
  "name": "starter-lib",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.29.1",
  "description": "A starter for creating a package.",
  "author": "leihaohao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/starter-lib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l246804/starter-lib.git"
  },
  "bugs": {
    "url": "https://github.com/l246804/starter-lib/issues"
  },
  "keywords": [],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write --cache --cache-location ./node_modules/.cache/.prettier-cache",
    "lint": "eslint . --cache --cache-location ./node_modules/.cache/.eslint-cache",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@sxzz/prettier-config": "^2.3.1",
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.12",
    "@vue/tsconfig": "^0.8.1",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.4.0",
    "happy-dom": "^20.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
